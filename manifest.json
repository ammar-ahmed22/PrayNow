{
    "manifest_version": 2,

    "name": "PrayNow",
    "description": "Prayer times and notifications in your browser",
    "version": "0.1.0",
    "browser_action": {
        "default_popup": "athan.html",
        "default_icon": "images/PrayNowIcon.png"
    },
    "permissions":[
        "activeTab", 
        "tabs",
        "storage"
    ]

    
}